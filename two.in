% identify the distinct numbers satisfying the equation TWO + TWO = FOUR
% where the letters represent digits from 0 to 9

assign(max_models,-1).
assign(domain_size,9).
set(arithmetic).

list(distinct).
  [T,W,O,F,U,R].
  [T,F,0].
end_of_list.

formulas(assumptions).
  T * 200 + W * 20 + 2 * O = 1000 * F + 100 * O + 10 * U + R.
end_of_list.